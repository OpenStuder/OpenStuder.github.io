{
  "$schema": "http://json-schema.org/draft/2019-09/schema#",
  "title": "PROPERTIES UNSUBSCRIBED body",
  "description": "List of IDs of the properties and their unsubscription status",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "status": {
        "type": "string",
        "enum": [
          "Success",
          "NoProperty",
          "Error"
        ]
      },
      "id": {
        "type": "string"
      }
    },
    "required": [
      "status",
      "id"
    ],
    "additionalProperties": false
  }
}
